package com.hcis.ipanther.common.privilege.exception;

public class PermissionException extends PrivilegeException
{
  private static final long serialVersionUID = 1L;

  public PermissionException()
  {
  }

  public PermissionException(String msg, Throwable ex)
  {
    super(msg, ex);
  }

  public PermissionException(String msg) {
    super(msg);
  }

  public PermissionException(Throwable ex) {
    super(ex);
  }
}